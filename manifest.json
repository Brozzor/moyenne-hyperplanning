{
  "manifest_version": 2,
  "name": "Moyenne-HP",
  "description": "Calcule la moyenne de vos notes sur hyper planning",
  "version": "0.0.1",
  "permissions": [
    "https://*.ynov.com/*",
    "http://*.ynov.com/*"
  ],
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.ynov.com/*",
        "http://*.ynov.com/*"
      ],
      "js": ["js/extension.js"]
    }
  ],
  "icons": {
    "256": "img/icon.png"
  }
}
